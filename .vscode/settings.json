{
  "solidity.soliumRules": {
      "quotes": ["error", "double"],
      "no-empty-blocks": "off",
      "indentation": ["error", 4],
      "max-len": ["warning", 79],
      "no-constant": ["error"],
      "security/enforce-explicit-visibility": ["error"],
      "security/no-block-members": ["warning"],
      "security/no-inline-assembly": ["warning"]
  },
  "editor.tabSize": 2,
}